{
  "name": "JsonLint",
  "displayName": "JsonLint",
  "description": "JsonLint is the JSON linter and validator",
  "version": "0.0.7",
  "publisher": "wekex",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/wekex35/JsonLint"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Linters","Formatters"
  ],
  "activationEvents": [
    "onCommand:JsonLint.launch",
    "onCommand:JsonLint.launchFromClipBoard"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "JsonLint.launch",
        "title": "JsonLint {}"
      },
      {
        "command": "JsonLint.launchFromClipBoard",
        "title": "JsonLint {} - From clipboard"
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "os": "^0.1.2",
    "path": "^0.12.7",
    "ps": "^1.0.0",
    "react-ace": "^10.1.0",
    "react-simple-code-editor": "^0.13.1"
  },
  "keywords": [
    "JsonLint",
    "json",
    "validator",
    "beautify",
    "formatter",
    "linter"
  ]
}
